# wealist Dev Environment - Parameter Store Variables
# 이 파일을 terraform.tfvars로 복사하고 실제 값을 입력하세요
# terraform.tfvars는 .gitignore에 추가되어 있어 Git에 커밋되지 않습니다

# AWS Region
aws_region = "ap-northeast-2"

# ============================================
# Passwords & Secrets
# ============================================

# PostgreSQL Superuser Password
postgres_superuser_password = "YOUR_SECURE_POSTGRES_PASSWORD_HERE"

# User Service Database Password
user_db_password = "YOUR_SECURE_USER_DB_PASSWORD_HERE"

# Board Service Database Password
board_db_password = "YOUR_SECURE_BOARD_DB_PASSWORD_HERE"

# Redis Password
redis_password = "YOUR_SECURE_REDIS_PASSWORD_HERE"

# JWT Secret (64+ characters for HS512)
jwt_secret = "YOUR_JWT_SECRET_AT_LEAST_64_BYTES_FOR_HS512_ALGORITHM_SECURITY"

# Google OAuth Client ID
google_client_id = "YOUR_GOOGLE_CLIENT_ID.apps.googleusercontent.com"

# Google OAuth Client Secret
google_client_secret = "YOUR_GOOGLE_CLIENT_SECRET"

# Grafana Admin Password
grafana_admin_password = "YOUR_SECURE_GRAFANA_PASSWORD_HERE"

# ============================================
# Usernames & Database Names (Optional - has defaults)
# ============================================

# PostgreSQL Superuser (default: postgres)
postgres_superuser = "postgres"

# User Service Database Name (default: wealist_user_db)
user_db_name = "wealist_user_db"

# User Service Database Username (default: wealist_user)
user_db_user = "wealist_user"

# Board Service Database Name (default: wealist_board_db)
board_db_name = "wealist_board_db"

# Board Service Database Username (default: wealist_board)
board_db_user = "wealist_board"

# Grafana Admin Username (default: admin)
grafana_admin_user = "admin"
